<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net7.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<UseWindowsForms>true</UseWindowsForms>
		<ApplicationIcon>multitool.ico</ApplicationIcon>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="ObjectExtensions.cs" />
	</ItemGroup>

	<ItemGroup>
	  <None Remove="Icons\multitool.ico" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="ObjectDumper.NET" Version="4.1.3" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="Icons\multitool.ico">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Include="multitool.ico" />
	  <Content Include="ObjectExtensions.cs">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

</Project>
